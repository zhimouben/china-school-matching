{
  "name": "china-school-matching",
  "version": "1.0.0",
  "private": true,
  "description": "A web crawler and matching system for international students in China",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
